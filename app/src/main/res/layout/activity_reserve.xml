<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReserveActivity">

    <ImageButton
        android:id="@+id/btnseat12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="0dp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/btnseat11"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9"
        app:srcCompat="@drawable/seat" />

    <ImageButton
        android:id="@+id/btnseat11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/seat"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton8"
        tools:layout_editor_absoluteX="244dp" />

    <ImageButton
        android:id="@+id/btnseat7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="414dp" />

    <ImageButton
        android:id="@+id/btnseat8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/seat"
        app:layout_constraintLeft_toRightOf="@+id/btnseat7"
        android:layout_marginLeft="0dp"
        tools:layout_editor_absoluteY="414dp" />

    <ImageButton
        android:id="@+id/btnseat9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/seat"
        app:layout_constraintLeft_toRightOf="@+id/btnseat8"
        android:layout_marginLeft="0dp"
        tools:layout_editor_absoluteY="414dp" />

    <ImageButton
        android:id="@+id/btnseat10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/btnseat4"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteX="188dp" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="367dp" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="367dp" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/btnseat2"
        app:layout_constraintStart_toEndOf="@+id/imageButton11"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="360dp" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/imageButton8"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="308dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/btnseat4"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="308dp" />

    <ImageButton
        android:id="@+id/btnseat4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/btnseat3"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="308dp" />

    <ImageButton
        android:id="@+id/btnseat3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/btnseat2"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="308dp" />

    <ImageButton
        android:id="@+id/btnseat2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="4dp"
        app:layout_constraintLeft_toRightOf="@+id/btnseat1"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="308dp" />

    <EditText
        android:id="@+id/txtdetail_type"
        android:layout_width="278dp"
        android:layout_height="37dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="75dp"
        android:ems="10"
        android:hint="Activity type"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtdetail_name"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <EditText
        android:id="@+id/txtdetail_time"
        android:layout_width="125dp"
        android:layout_height="38dp"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:hint="Time"
        android:inputType="textPersonName"
        app:layout_constraintTop_toBottomOf="@+id/txtdetail_act"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/txtddetail_date"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.365" />

    <EditText
        android:id="@+id/txtddetail_date"
        android:layout_width="125dp"
        android:layout_height="38dp"
        android:layout_marginLeft="42dp"
        android:ems="10"
        android:hint="Date"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/txtdetail_name" />

    <EditText
        android:id="@+id/txtdetail_act"
        android:layout_width="125dp"
        android:layout_height="38dp"
        android:layout_marginTop="13dp"
        android:ems="10"
        android:hint="Activity"
        android:inputType="textPersonName"
        app:layout_constraintTop_toBottomOf="@+id/txtjoin"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/txtdetail_name"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.365" />

    <TextView
        android:id="@+id/txtjoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text=" Join "
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtdetail_name"
        android:layout_width="125dp"
        android:layout_height="38dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="13dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtjoin" />

    <ImageButton
        android:id="@+id/btnseat1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/seat"
        tools:layout_editor_absoluteY="308dp" />

    <Button
        android:id="@+id/btnconfirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        android:text="Confirm"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnseat9" />

    <TextView
        android:id="@+id/txtstage"
        android:layout_width="294dp"
        android:layout_height="29dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:background="@android:color/black"
        android:text="Stage"
        android:textColor="@android:color/background_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="263dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/seat"
        app:layout_constraintStart_toEndOf="@+id/btnseat9"
        tools:layout_editor_absoluteY="412dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/seat"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        tools:layout_editor_absoluteY="412dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/seat"
        app:layout_constraintStart_toEndOf="@+id/imageButton2"
        tools:layout_editor_absoluteY="412dp" />

</android.support.constraint.ConstraintLayout>
